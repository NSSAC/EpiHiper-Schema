        "operation": "intersection",
        "sets": [
          {
            "elementType": "node",
            "left": {
              "node": {"property": "healthState"}
            },
            "operator": "==",
            "right": {
              "value": {"healthState": "Isymp"}
            }
          },
          {
            "elementType": "node",
            "left": {
              "node": {
                "property": "nodeTrait",
                "feature": "enteredIsymp"
              }
            },
            "operator": "==",
            "right": {
              "value": {
                "trait": "nodeTrait",
                "feature": "enteredIsymp",
                "enum": "false"
              }
            }
          }
        ]
      }
    },
    {
      "id": "schoolChildrenEnteredIsymp",
